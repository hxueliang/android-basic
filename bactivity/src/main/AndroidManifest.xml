<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidBasic">
        <activity
            android:name=".GResponseActivity"
            android:exported="false" />
        <activity
            android:name=".FReceiveActivity"
            android:exported="false" />
        <activity
            android:name=".CLoginSuccessActivity"
            android:exported="true" />
        <activity
            android:name=".BSecondActivity"
            android:exported="false" />
        <activity
            android:name=".AFinishActivity"
            android:exported="true" />
        <activity
            android:name=".JShortcutsActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <!--
                元数据：可以用来存放第三方SDK注册的token等信息
                如：高德地图、友盟、微信登录
            -->
            <meta-data
                android:name="myName"
                android:value="HuangXL" />

            <meta-data
                android:name="android.app.shortcuts"
                android:resource="@xml/shortcuts" />
        </activity>
    </application>

</manifest>